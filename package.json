{
  "name": "yitianyibu",
  "version": "0.0.1",
  "description": "Yves homepage",
  "main": "app.js",
  "scripts": {
    "start": "NODE_ENV=development npm run dev",
    "dev": "nodemon src/app.js --exec babel-node",
    "build": "npm run build:server",
    "build:server": "babel src -d dist",
    "server": "NODE_ENV=production node dist/app.js",
    "test": "npm run test:lint",
    "test:lint": "npm run test:srclint",
    "test:srclint": "eslint src client --ext '.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coolfishstudio/yitianyibu.git"
  },
  "keywords": [
    "Yves"
  ],
  "author": "Yves",
  "precommit": [
    "test"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/coolfishstudio/yitianyibu/issues"
  },
  "homepage": "https://github.com/coolfishstudio/yitianyibu#readme",
  "dependencies": {
    "body-parser": "^1.16.0",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.0",
    "pug": "^2.0.0-beta6"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "eslint": "^3.14.0",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-import": "^2.2.0",
    "nodemon": "^1.11.0"
  }
}
