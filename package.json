  },
  "homepage": "https://github.com/coolfishstudio/yitianyibu#readme",
  "dependencies": {
    "body-parser": "^1.16.0",
    "bytes": "^2.4.0",
    "connect-busboy": "0.0.2",
    "cookie-parser": "^1.4.3",
    "dotenv": "^4.0.0",
    "express": "^4.14.0",
    "express-session": "^1.15.1",
    "formidable": "^1.1.1",
    "log4js": "^1.1.0",
